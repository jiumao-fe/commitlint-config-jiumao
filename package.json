{
  "name": "@jiumao/commitlint-config",
  "version": "0.0.1",
  "description": "JiuMao前端团队CommitLint配置",
  "main": "index.js",
  "files": [
    "index.js",
    "README.md"
  ],
  "scripts": {
    "bootstrap": "yarn || npm i",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "repository": "https://github.com/jiumao-fe/commitlint-config-jiumao.git",
  "author": "wangxingkang <wangxingkang@sensoro.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "npm run commitmsg"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "husky": "^1.1.3"
  }
}
